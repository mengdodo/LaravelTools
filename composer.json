{
    "name": "mengdodo/laravel-tools",
    "description": "laravel develop tools. Create service,trait,Repositories,Presenters quickly, support ^5.5 and 6",
    "keywords": ["laravel","develop tools","laravel-tools"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://mengdodo.com",
    "authors": [
        {
            "name": "mengdodo",
            "email": "dodo@mengdodo.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "^5|^6"
    },
    "autoload": {
        "psr-4": {
            "Mengdodo\\LaravelTools\\": "src"
        }
    },
    "autoload-dev": {

    },
    "extra": {
        "laravel": {
            "providers": [
                "Mengdodo\\LaravelTools\\LaravelToolsServiceProvider"
            ],
            "aliases": {
                "LaravelToolsFacade": "Mengdodo\\LaravelTools\\Facade\\LaravelToolsFacade"
            }
        }
    }
}
