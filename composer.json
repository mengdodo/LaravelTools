{
    "name": "mengdodo/laravel-tools",
    "description": "laravel develop artisan command tools. Create service,trait,Repositories,Presenters template quickly, support ^5.5 and 6、7、8、9",
    "keywords": ["laravel","command tools","mengdodo"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://www.mengdodo.com",
    "authors": [
        {
            "name": "mengdodo",
            "email": "dodo@mengdodo.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0||^8.0",
        "laravel/framework": "^6.20.14||^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mengdodo\\LaravelTools\\": "src/"
        }
    },
    "autoload-dev": {

    },
    "extra": {
        "laravel": {
            "providers": [
                "Mengdodo\\LaravelTools\\LaravelToolsServiceProvider"
            ],
            "aliases": {
                "LaravelToolsFacade": "Mengdodo\\LaravelTools\\Facade\\LaravelToolsFacade"
            }
        }
    }
}
